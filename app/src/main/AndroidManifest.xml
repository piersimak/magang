<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="diginetmedia.com.sikedes">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.SearchResultActivity"
            android:label="tes">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.app.default_searchable"
                android:value=".SearchResultActivity" />
        </activity>
        <activity android:name=".activity.ListDataKemiskinan" />
        <activity android:name=".activity.DeskripsiDataKemiskinan" />
        <activity android:name=".activity.UpdateDataKemiskinan" />
        <activity android:name=".activity.VerifikasiDataKemiskinan" />
        <activity android:name=".activity.VerifikasiDataMasyarakat" />
        <activity android:name=".activity.DetailLaporan" />
        <activity android:name=".activity.KeranjangActivity" />
        <activity android:name=".activity.DataKemiskinanYangDibantu" />
        <activity android:name=".activity.DetailDataKemiskinanYangDibantu" />
        <activity
            android:name=".Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.SplashActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.ListDusunActivity"></activity>
    </application>

</manifest>